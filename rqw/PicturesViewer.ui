QListView::item:selected {
    background: #666666;
    color: #FFFFFF;
    border: 1px solid #555555;
}

/* 焦点状态 */
QListView::item:selected:active {
    background: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #666666, 
        stop:1 #555555
    );
}

/* 禁用状态 */
