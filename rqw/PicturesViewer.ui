    padding: 2px 6px;
}

/* 滚动条样式 */
QScrollBar:vertical {
    width: 10px;
    background: #FFFFFF;
}

QScrollBar::handle:vertical {
    background: #CCCCCC;
    border-radius: 4px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: #AAAAAA;
}

QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    height: 0px;
}

/* 强调型变体 */
QListView.emphasized::item:selected {
    background: #4A4A4A;
    border-color: #3A3A3A;
}

/* 交替行颜色 */
QListView::item:alternate {
    background: #FAFAFA;
}

/* 角部处理 */
QScrollBar #qt_scrollarea_corner {
    background: #FFFFFF;
}</string>
